   </component>
   <component name="CEUniqueID" class="StringElement" insert="true">
       <property name="Value">
           <column name="CEUniqueID" not-null="true" default="''" />
       </property>
   </component>
   <component name="VOcorrid" class="IntegerElement" insert="true">
       <property name="Value">
