 * @param context
 *            IChartScriptContext
 */
function beforeDrawAxisTitle( axis, label, context )
{
	rep = context.getExternalContext().getScriptable();
	val1 =rep.getParameterValue(&quot;DisplayMetric&quot;);
	if(!axis.isCategoryAxis())
	{
		if(val1==&quot;Cpu&quot;)
		{
		label.getCaption().setValue(&quot;Process Hours&quot;);
		}
		else if(val1==&quot;WallDuration&quot;)
		{
		label.getCaption().setValue(&quot;Wallclock Hours&quot;);
		}
		else if(val1==&quot;Njobs&quot;)
		{
		label.getCaption().setValue(&quot;Job Count&quot;);
		}
		else {
		label.getCaption().setValue(&quot;***undefined metric&quot;);
