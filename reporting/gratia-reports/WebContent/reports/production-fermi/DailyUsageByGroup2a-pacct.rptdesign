  <Script>/**&#xD;
 * Called before generation of chart model to GeneratedChartState.&#xD;
 * &#xD;
 * @param chart&#xD;
 *            Chart&#xD;
 * @param icsc&#xD;
 *            IChartScriptContext&#xD;
 */&#xD;
&#xD;
function beforeGeneration(chart, icsc)&#xD;
{&#xD;
	rep = icsc.getExternalContext().getScriptable();&#xD;
	&#xD;
	metric = rep.getParameterValue(&quot;DisplayMetric&quot;);&#xD;
	if(metric==&quot;process&quot;) metric_text = &quot;Process Hours&quot;;&#xD;
	if(metric==&quot;wallclock&quot;) metric_text = &quot;Wallclock Hours&quot;;&#xD;
	if(metric==&quot;cpuuser&quot;) metric_text = &quot;User Hours&quot;;&#xD;
	if(metric==&quot;cpusystem&quot;) metric_text = &quot;System Hours&quot;;&#xD;
	&#xD;
	rep_title = rep.getParameterValue(&quot;ReportTitle&quot;) + &quot; (&quot; + metric_text + &quot;)&quot; ;&#xD;
	rep_subtitle = rep.getParameterValue(&quot;ReportSubtitle&quot;);&#xD;
	chart.getTitle().getLabel().getCaption().setValue(rep_title + &quot;\n&quot; + rep_subtitle);&#xD;
}</Script>
