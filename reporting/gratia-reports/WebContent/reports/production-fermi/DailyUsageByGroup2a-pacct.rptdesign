                </structure>
                <structure>
                    <property name="position">7</property>
                    <property name="name">WallDuration</property>
                    <property name="nativeName">WallDuration</property>
                    <property name="dataType">float</property>
                    <property name="nativeDataType">8</property>
                </structure>
            </list-property>
            <property name="queryText">select V.VOName as voname, 
  S.SiteName as sitename,
  date_format(V.EndTime,?) as datevalue, 
  sum(V.CpuUserDuration+V.CpuSystemDuration) as Cpu,
  sum(V.CpuUserDuration) as CpuUser,
  sum(V.CpuSystemDuration) as CpuSystem,
  sum(V.WallDuration) as WallDuration
from VOProbeSummary V, Site S, Probe P 
where V.ResourceType = 'RawCPU'
  and V.ProbeName = P.probename
  and P.siteid = S.siteid
  and V.VOName = ?
  and S.SiteName = ?
  and (V.EndTime >= ?  
  and V.EndTime &lt;= ?)
group by datevalue, sitename,  V.VOName
order by datevalue</property>
